extends layout

block content
    .container 
        .row
            .col-md-12
                img.center-block.img-responsive(src='/img/717w.JPG')
        hr
    .section
        .container
            .row
                .col-md-12
                    h1.text-center Student Login
            .row
                .col-md-offset-3.col-md-6
                    form(role='form')
                        .form-group
                            .input-group
                                input.form-control(type='text', placeholder="Enter Teacher's Room Code")
                                span.input-group-btn
                                    button.btn.btn-primary(type='button') Go
